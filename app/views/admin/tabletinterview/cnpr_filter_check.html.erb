    <script>
    $(function () {
      $('#filter_submit').click(function(){
        var select_filter = []
        $('input:checkbox[name="check_box"]').each(function() {
            if(this.checked){//checked 처리된 항목의 값
              if(this.value.indexOf("_all") <= 0){
                select_filter.push(this.value)
              }
            }
         });
         opener.selectFilter(select_filter);
         self.close();
      });

      $("input:checkbox[name='check_box']").prop("checked", true);
      $("input:checkbox[id='all_check']").prop("checked", true);

      $(document).on("click", "#before_solution_check", function (e) {
        if (!$("input:checkbox[id='before_solution_check']").is(":checked")){
          $("input:checkbox[class='before_solution_check']").prop("checked", false);
          $("#before_solution_check_detail").show();
        } else {
          $("input:checkbox[class='before_solution_check']").prop("checked", true);
          $("#before_solution_check_detail").hide();
        }
      });
      $(document).on("click", "#after_solution_check", function (e) {
        if (!$("input:checkbox[id='after_solution_check']").is(":checked")){
          $("input:checkbox[class='after_solution_check']").prop("checked", false);
          $("#after_solution_check_detail").show();
        } else {
