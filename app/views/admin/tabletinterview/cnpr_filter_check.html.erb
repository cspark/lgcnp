          $("input:checkbox[class='answer_check']").prop("checked", false);
          $("#answer_check_detail").show();
        } else {
          $("input:checkbox[class='answer_check']").prop("checked", true);
          $("#answer_check_detail").hide();
        }
      });
      $(document).on("click", "#graph_check", function (e) {
        if (!$("input:checkbox[id='graph_check']").is(":checked")){
          $("input:checkbox[class='graph_check']").prop("checked", false);
          $("#graph_check_detail").show();
        } else {
          $("input:checkbox[class='graph_check']").prop("checked", true);
          $("#graph_check_detail").hide();
        }
      });

      $(document).on("click", "#all_check", function (e) {
        if (!$("input:checkbox[id='all_check']").is(":checked")){
          $("input:checkbox[name='check_box']").prop("checked", false);
        } else {
          $("input:checkbox[name='check_box']").prop("checked", true);
        }
      });

    });
    </script>

