        } else {
          $("input:checkbox[class='recommand_program_step_check']").prop("checked", true);
          $("#recommand_program_step_check_detail").hide();
        }
      });
      $(document).on("click", "#purchase_check", function (e) {
        if (!$("input:checkbox[id='purchase_check']").is(":checked")){
          $("input:checkbox[class='purchase_check']").prop("checked", false);
          $("#purchase_check_detail").show();
        } else {
          $("input:checkbox[class='purchase_check']").prop("checked", true);
          $("#purchase_check_detail").hide();
        }
      });
      $(document).on("click", "#answer_check", function (e) {
