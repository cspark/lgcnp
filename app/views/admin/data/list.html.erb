          <option value="skin_type_jisung_boghab">지복합성</option>
          <option value="skin_type_boghab">복합성</option>
          <option value="skin_type_gunsung">건성</option>
          <option value="skin_type_jungsung">중성</option>
          <option value="skin_type_jisung">지성</option>
        </select>

        민감성 :
        <select class="form-control" id="select_senstive">
          <option value="all">전체</option>
          <option value="yes">예</option>
          <option value="no">아니오</option>
        </select>

        피부고민1 :
        <select class="form-control" id="select_skin_anxiety1">
