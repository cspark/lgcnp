    <script>
    $ (function () {
        <% user = Custinfo.where(custserial: @fcdata.custserial).first %>
        try
        {
          $('#name_<%= @fcdata.custserial.to_i %>').append( decode_utf8('<%= user.custname %>'));
        }
        catch(err)
        {
          $('#name_<%= @fcdata.custserial.to_i %>').append('<%= user.custname %>');
        }
      });

      function decode_utf8(s) {
        return decodeURI(s);
      }

    </script>
