wb = xlsx_package.workbook
wb.add_worksheet(name: "Fcdata") do |sheet|
    sheet.add_row ["이름","진단 날짜","채널","매장","분석 횟수"]
    @fcdatas_excel.each do |fcdata|
        custinfo = Custinfo.where(custserial: fcdata.custserial).first
        if !custinfo.custname.nil?
         custname = URI.decode(custinfo.custname)
        else
         custname = ""
        end

        sheet.add_row [custname, fcdata.measuredate, fcdata.ch_cd, fcdata.shop_cd, fcdata.measureno.to_i]
    end
end
