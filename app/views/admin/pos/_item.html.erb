      <script>
        $(function () {
          <% user = Custinfo.find data.custserial.to_i %>
          try
          {
            var userName = decode_utf8('<%= user.custname %>')
            tempUserName = ""
            for(var i = 0; i < userName.length; i++){
              if(i == 0)
                tempUserName += userName.slice(0, 1)
              else if(i == userName.length - 1)
                tempUserName += userName.slice(userName.length - 1, userName.length)
              else
                tempUserName += "*"
            }
            $('#name_fcpos_<%= data.custserial.to_i %>_<%= data.measureno.to_i %>').append(tempUserName);
          }
          catch(err)
          {
            $('#name_fcpos_<%= data.custserial.to_i %>_<%= data.measureno.to_i %>').append('<%= user.custname %>');
          }
        });

        function decode_utf8(s) {
          try{
            return decodeURI(s);
          }catch(err){
            console.log(err);
            return s;
          }
        }
      </script>
    </head>
