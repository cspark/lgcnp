<!DOCTYPE html>
<html>
    <head>
      <script type="text/javascript" src="../js/jquery.js"></script>
      <script type="text/javascript" src="../js/bootstrap.js"></script>

      <link href="../css/bootstrap.min.css" rel="stylesheet">

      <link rel="stylesheet" href="https://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" type="text/css"/>
      <script src="https://code.jquery.com/ui/1.10.0/jquery-ui.min.js"></script>

      <script>
        $(function () {
          <% user = Custinfo.where(custserial: data.custserial).first %>
          <% if !user.nil? %>
          try
          {
            var userName = decode_utf8('<%= user.custname %>')
            tempUserName = ""
            if (userName.length == 2){
              tempUserName += userName.slice(0, 1)
              tempUserName += "*"
            } else {
              for(var i = 0; i < userName.length; i++){
                if(i == 0)
                  tempUserName += userName.slice(0, 1)
                else if(i == userName.length - 1)
                  tempUserName += userName.slice(userName.length - 1, userName.length)
                else
                  tempUserName += "*"
              }
            }
            $('#name_fcpos_<%= data.custserial.to_i %>_<%= data.measureno.to_i %>').append(tempUserName);
          }
          catch(err)
          {
            $('#name_fcpos_<%= data.custserial.to_i %>_<%= data.measureno.to_i %>').append('<%= user.custname %>');
          }
          <% end %>
        });

        function decode_utf8(s) {
          try{
            return decodeURI(s);
          }catch(err){
            console.log(err);
            return s;
          }
        }
      </script>
    </head>
    <body>
      <% custinfo = Custinfo.where(custserial: data.custserial).first %>
      <% if !custinfo.nil? %>
        <tr class="table_tr text-center" id="<%= data.custserial.to_i %>/<%= data.measureno %>">
